<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Company Config for Export QUADRA module -->
    <record id="res_company_export_quadra" model="ir.ui.view">
        <field name="name">res.company.exportquadra.config</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='currency_id']" position="after">
                <field name="expc_testing"/>
            </xpath>
        </field>
    </record>
</odoo>